{
  "name": "privateinsight",
  "version": "1.0.0",
  "description": "AI-Powered Privacy-Preserving Data Analytics Platform on Metis Hyperion + Filecoin",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:filecoin": "ts-node --esm scripts/deploy-filecoin.ts",
    "deploy:metis": "ts-node --esm scripts/deploy-metis.ts",
    "deploy:all": "bun run deploy:filecoin && bun run deploy:metis",
    "setup:pdp": "ts-node --esm scripts/setup-pdp.ts",
    "setup:filcdn": "ts-node --esm scripts/setup-filcdn.ts",
    "setup:bridge": "ts-node --esm scripts/setup-bridge.ts",
    "verify:filecoin": "ts-node --esm scripts/verify-filecoin.ts",
    "verify:metis": "ts-node --esm scripts/verify-metis.ts",
    "agents:start": "ts-node --esm ai-agents/src/index.ts",
    "agents:build": "tsc -p ai-agents/tsconfig.json",
    "privacy:test": "ts-node --esm tests/privacy-tests.ts",
    "zk:setup": "ts-node --esm scripts/setup-zk.ts",
    "contracts:compile": "echo 'Contract compilation - implementation pending'",
    "contracts:test": "echo 'Contract testing - implementation pending'",
    "db:migrate": "echo 'Database migration - implementation pending'",
    "db:generate": "echo 'Database generation - implementation pending'",
    "docker:build": "docker build -t privateinsight .",
    "docker:run": "docker run -p 3000:3000 privateinsight"
  },
